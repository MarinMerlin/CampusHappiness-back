{
    "extends": "airbnb",
    "rules":{
        "quotes": 0,
        "eol-last": 0,
        "func-names": 0,
        "camelcase": 0,
        "no-plusplus": 0,
        "no-console": 0,
        "consistent-return": 0,
        "prefer-destructuring": 0,
        "object-shorthand": 0,
        "no-param-reassign": 0,
        "no-trailing-spaces": 0,
        "import/no-extraneous-dependencies": 0,
        "prefer-arrow-callback": 0,
        "no-case-declarations": 0
    }
}